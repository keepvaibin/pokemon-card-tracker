{
  "name": "pokemon-card-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate:user": "prisma generate --schema=prisma/users/schema.prisma",
    "prisma:generate:cards": "prisma generate --schema=prisma/cards/schema.prisma",
    "prisma:generate:timescale": "prisma generate --schema=prisma/timescale/schema.prisma",
    "prisma:generate": "prisma generate --schema=prisma/timescale/schema.prisma && prisma generate --schema=prisma/cards/schema.prisma && prisma generate --schema=prisma/users/schema.prisma",
    "prisma:deploy": "prisma migrate deploy --schema=prisma/timescale/schema.prisma && prisma migrate deploy --schema=prisma/cards/schema.prisma && prisma migrate deploy --schema=prisma/users/schema.prisma",
    "prisma:seed": "ts-node prisma/users/seed.ts",
    "postinstall": "npm run prisma:generate"
  },
  "prisma": {
    "seed": "ts-node prisma/users/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.13.0",
    "axios": "^1.11.0",
    "framer-motion": "^12.23.12",
    "google-auth-library": "^10.2.1",
    "next": "15.4.5",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "prisma": "^6.13.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-sparklines": "^1.7.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
